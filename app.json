{
  "expo": {
    "scheme": "1easyTravel",
    "name": "1EasyTravel",
    "slug": "1EasyTravel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashimage.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splashimage.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bajdev.1easytravel",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show nearby rides and services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/images/splashimage.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
